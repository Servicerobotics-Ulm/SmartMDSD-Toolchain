        <requirement
            name="org.eclipse.xtext.sdk.feature.group"/>
        <sourceLocator
            rootFolder="${git.clone.ROSMixedPort.github.location}"
            locateNestedProjects="true">
          <excludedPath>de.seronet_projekt.ros.componentGateway.generator</excludedPath>
          <excludedPath>ROS-MixedPort-Examples</excludedPath>
        </sourceLocator>
        <sourceLocator
            rootFolder="${git.clone.SmartMDSD.github.location}"
            locateNestedProjects="true"/>
        <repositoryList
            name="Oxygen">
          <repository
              url="http://download.eclipse.org/releases/oxygen"/>
        </repositoryList>
        <repositoryList
