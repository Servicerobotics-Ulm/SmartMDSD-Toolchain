    <workingSet
        name="org.xtext.grammars"
        id="org.xtext.grammars">
      <predicate
          xsi:type="predicates:NamePredicate"
          pattern="org.xtext.*"/>
    </workingSet>
    <workingSet
        name="org.xtend.generators"
        id="org.xtend.generators">
      <predicate
          xsi:type="predicates:OrPredicate">
        <operand
            xsi:type="predicates:NamePredicate"
            pattern="org.xtend.*"/>
        <operand
            xsi:type="predicates:NamePredicate"
            pattern="org.open62541.xml.compiler"/>
