               style="push">
            <parameter
                  name="org.eclipse.smartmdsd.ui.natureParameter"
                  value="org.eclipse.smartmdsd.ui.natures.DomainModelsNature">
            </parameter>
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt
                        type="org.eclipse.core.resources.IProject">
                     <and>
                        <not>
                           <test
                                 property="org.eclipse.core.resources.projectNature"
                                 value="org.eclipse.smartmdsd.ui.natures.DomainModelsNature">
                           </test>
                        </not>
                        <test
                              property="org.eclipse.core.resources.open"
                              value="true">
                        </test>
                        <with
                              variable="activeWorkbenchWindow.activePerspective">
                           <or>
                              <equals
                                    value="org.eclipse.smartmdsd.ui.perspectives.domainExpert">
                              </equals>
                              <equals
