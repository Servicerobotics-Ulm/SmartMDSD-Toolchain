         point="org.eclipse.ui.commands">
      <category
            id="org.eclipse.smartmdsd.ui.commands"
            name="SmartMDSD Commands">
      </category>
      <command
            categoryId="org.eclipse.smartmdsd.ui.commands"
            description="Automated Code Generation"
            id="org.eclipse.smartmdsd.ui.autocodegen"
            name="AutoCodeGeneration">
         <state
               class="org.eclipse.ui.handlers.RegistryToggleState:true"
               id="org.eclipse.ui.commands.toggleState">
         </state>
      </command>
      <command
            categoryId="org.eclipse.smartmdsd.ui.commands"
            description="Run Code-Generation"
            id="org.eclipse.smartmdsd.ui.codeGeneratorTrigger"
            name="RunCodeGeneration">
      </command>
      <command
