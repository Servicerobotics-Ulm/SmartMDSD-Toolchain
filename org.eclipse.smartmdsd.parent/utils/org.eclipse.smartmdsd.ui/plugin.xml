      </handler>
      <handler
            class="org.eclipse.smartmdsd.ui.deployment.DeploymentActionHandler"
            commandId="org.eclipse.smartmdsd.ui.deployment">
      </handler>
      <handler
            class="org.eclipse.smartmdsd.ui.linux.OpenDebianTerminalCommand"
            commandId="org.eclipse.smartmdsd.ui.terminal">
      </handler>
