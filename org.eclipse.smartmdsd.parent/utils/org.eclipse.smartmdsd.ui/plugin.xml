            description="Toggle SmartMDSD Project Nature"
            id="org.eclipse.smartmdsd.ui.toggleProjectNature"
            name="Toggle SmartMDSD Project Nature">
         <commandParameter
               id="org.eclipse.smartmdsd.ui.natureParameter"
               name="NatureID"
               optional="false">
         </commandParameter>
      </command>
      <command
            categoryId="org.eclipse.smartmdsd.ui.commands"
            description="Deployment Action"
            id="org.eclipse.smartmdsd.ui.deployment"
            name="Deploy">
      </command>
      <command
            categoryId="org.eclipse.smartmdsd.ui.commands"
            description="Open Resource in Debian Terminal"
            id="org.eclipse.smartmdsd.ui.terminal"
            name="Terminal">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
